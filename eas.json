{
  "cli": {
    "version": ">= 16.18.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "env": {
        "EXPO_PUBLIC_DEBUG_UI": "1",
        "EXPO_PUBLIC_SHOW_DEBUG_PANELS": "1"
      },
      "distribution": "internal",
      "credentialsSource": "local",
      "android": { "buildType": "apk" }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_DEBUG_UI": "1"
      },
      "distribution": "store",
      "credentialsSource": "local",
      "android": { "buildType": "app-bundle" }
    }
  },
  "submit": {
    "production": {}
  }
}
